{
  "env": {
    "browser": true
  },
  "rules": {
    "unicorn/prefer-node-protocol": "error",
    "react/jsx-key": "off",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "eslint/no-console": ["error", {"allow": ["debug", "info", "warn", "error"]}]
  }
}
