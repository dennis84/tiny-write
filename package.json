{
  "name": "TinyWrite",
  "version": "0.5.1",
  "build": {
    "artifactName": "TinyWrite.${ext}",
    "appId": "com.github.dennis84.tinywrite",
    "files": [
      "index.js",
      "preload.js",
      "dist/**",
      "!dist/app"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    },
    "directories": {
      "output": "dist/app"
    }
  },
  "dependencies": {
    "@codemirror/commands": "^0.18.1",
    "@codemirror/highlight": "^0.18.3",
    "@codemirror/lang-cpp": "^0.18.0",
    "@codemirror/lang-css": "^0.18.0",
    "@codemirror/lang-html": "^0.18.0",
    "@codemirror/lang-java": "^0.18.0",
    "@codemirror/lang-javascript": "^0.18.0",
    "@codemirror/lang-json": "^0.18.0",
    "@codemirror/lang-markdown": "^0.18.1",
    "@codemirror/lang-python": "^0.18.0",
    "@codemirror/lang-rust": "^0.18.0",
    "@codemirror/lang-sql": "^0.18.0",
    "@codemirror/lang-xml": "^0.18.0",
    "@codemirror/legacy-modes": "^0.18.0",
    "@codemirror/lint": "^0.18.1",
    "@codemirror/state": "^0.18.5",
    "@codemirror/stream-parser": "^0.18.1",
    "@codemirror/view": "^0.18.6",
    "@ddietr/codemirror-themes": "^1.0.1",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "date-fns": "^2.20.1",
    "devicon": "devicons/devicon#master",
    "electron-updater": "^4.3.8",
    "file-type": "^16.3.0",
    "idb": "^6.0.0",
    "markdown-it": "^12.0.4",
    "prettier": "^2.2.1",
    "prosemirror-collab": "^1.2.2",
    "prosemirror-commands": "^1.1.7",
    "prosemirror-dropcursor": "^1.3.4",
    "prosemirror-example-setup": "^1.1.2",
    "prosemirror-gapcursor": "^1.1.5",
    "prosemirror-history": "^1.1.3",
    "prosemirror-inputrules": "^1.1.3",
    "prosemirror-keymap": "^1.1.4",
    "prosemirror-markdown": "^1.5.1",
    "prosemirror-model": "^1.14.0",
    "prosemirror-schema-list": "^1.1.4",
    "prosemirror-state": "^1.3.4",
    "prosemirror-transform": "^1.3.2",
    "prosemirror-view": "^1.18.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "socket.io-client": "^4.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vitejs/plugin-react-refresh": "^1.3.2",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "eslint": "^7.24.0",
    "eslint-plugin-react": "^7.23.2",
    "jest": "^26.6.3",
    "jest-playwright-preset": "^1.5.1",
    "playwright": "^1.10.0",
    "ts-jest": "^26.5.4",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "vite": "^2.1.5"
  },
  "hasInstallScript": true,
  "scripts": {
    "web": "vite",
    "app": "NODE_ENV=dev electron . & vite",
    "build": "npm run lint && vite build --base './'",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
